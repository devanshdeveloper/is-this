{
  "name": "@devanshdeveloper/is-this",
  "version": "1.1.7",
  "description": "A lightweight JavaScript utility for type checking.",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "scripts": {
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "build": "npm run build:cjs && npm run build:esm",
    "watch:cjs": "tsc --outDir dist/cjs --module commonjs --watch",
    "watch:esm": "tsc --outDir dist/esm --module esnext --watch",
    "watch": "npm run watch:esm & npm run watch:cjs",
    "publish": "git add . && git commit -m /\"code updated\"/ && git push && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devanshdeveloper/is-this.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "validation",
    "utility",
    "type",
    "check",
    "helper",
    "data",
    "type-check",
    "data-validation",
    "helper-functions",
    "type-utils",
    "data-types",
    "check-library"
  ],
  "author": "Devansh Khetwani",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.7.5",
    "typescript": "^5.0.0"
  }
}
